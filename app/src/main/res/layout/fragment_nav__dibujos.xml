<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fate"
    tools:context=".nav_Dibujos2">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:videoId="hJDcWgZcPzg"
                app:autoPlay="false"
                app:showFullScreenButton="true" />

            <TextView
                android:id="@+id/textView2"
                style="@android:style/Widget.TextView"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:fontFamily="casual"
                android:text="Seleccione para ver el Video"
                android:textSize="20dp"
                android:textStyle="normal|bold|italic" />

            <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:videoId="RnKa1RP5fgU"
                app:autoPlay="false"
                app:showFullScreenButton="true" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:paddingVertical="5dp"
                android:src="@drawable/nezuko" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:paddingVertical="5dp"
                android:src="@drawable/ganyu" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:paddingVertical="5dp"
                android:src="@drawable/hina" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:paddingVertical="5dp"
                android:src="@drawable/theresa" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="250dp"
                android:paddingVertical="10dp"
                android:src="@drawable/miku" />

            <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:videoId="HjfWMaZZ7VI"
                app:autoPlay="false"
                app:showFullScreenButton="true" />

        </LinearLayout>
    </ScrollView>
</FrameLayout>